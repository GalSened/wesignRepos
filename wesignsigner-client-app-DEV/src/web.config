<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <rewrite>
            <!--  <rewriteMaps>
            <rewriteMap name="^(.*)$" />
        </rewriteMaps>
        -->
            <rules>
                <!--<rule name="http redirect" stopProcessing="true">
				<match url="(.*)" />
				<conditions>
					<add input="{HTTPS}" pattern="^OFF$" />
				</conditions>
				<action type="Redirect" url="https://{HTTP_HOST}{REQUEST_URI}" />
			</rule>
            -->
                <rule name="Angular Route signer" stopProcessing="true">
                    <match url=".*" />
                    <conditions logicalGrouping="MatchAll">
                        <add input="{REQUEST_URI}" pattern="/UserApi(.*)$" negate="true" />
                        <add input="{REQUEST_URI}" pattern="/SignerApi(.*)$" negate="true" />
                      <!--  <add input="{REQUEST_URI}" pattern="/Signer(.*)$" negate="true" />-->
                        <add input="{REQUEST_URI}" pattern="/Old(.*)$" negate="true" />
                        <add input="{REQUEST_URI}" pattern="/PDFConverter(.*)$" negate="true" />
                        <add input="{REQUEST_FILENAME}" matchType="IsFile" negate="true" />
                        <add input="{REQUEST_FILENAME}" matchType="IsDirectory" negate="true" />
                    </conditions>
                    <action type="Rewrite" url="./index.html" />
                </rule>

            </rules>
        </rewrite>
        <!-- Remove X-Powered-By header 
            Remove Server header
<remove name="X-Powered-By" />
<remove name="Server" />
</customHeaders>
</httpProtocol>
<security>
 
<requestFiltering removeServerHeader ="true" />
</security>-->

    </system.webServer>
</configuration>
